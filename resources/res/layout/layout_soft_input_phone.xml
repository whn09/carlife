<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="horizontal" android:background="@drawable/com_shape_dialog_bg" android:layout_width="wrap_content" android:layout_height="match_parent">
    <ListView android:id="@+id/lv_sug" android:focusable="true" android:visibility="gone" android:scrollbars="none" android:layout_width="180dp" android:layout_height="wrap_content" android:layout_marginTop="8dp" android:layout_marginBottom="8dp" android:listSelector="@drawable/com_bg_item_selector" android:divider="@null"/>
    <View android:id="@+id/temp1" android:background="@color/cl_line_a1_item" android:layout_width="@dimen/common_item_line" android:layout_height="match_parent"/>
    <RelativeLayout android:id="@+id/layout_right" android:layout_width="@dimen/common_menu_right_width" android:layout_height="wrap_content">
        <RelativeLayout android:id="@+id/layout_left_title" android:layout_width="wrap_content" android:layout_height="64dp">
            <ImageButton android:id="@+id/ib_close" android:background="@drawable/common_btn_bg_focus" android:visibility="gone" android:layout_width="@dimen/common_icon_small" android:layout_height="64dp" android:src="@drawable/com_ic_close_selector"/>
            <ImageButton android:id="@+id/btn_delete" android:background="@drawable/com_bg_btn_selector" android:visibility="invisible" android:layout_width="@dimen/common_icon_small" android:layout_height="@dimen/common_icon_small" android:src="@drawable/input_ic_delete" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            <EditText android:textSize="26sp" android:textColor="@color/cl_other_c_highlight" android:ellipsize="start" android:gravity="center" android:id="@+id/et_input" android:background="@null" android:focusable="false" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true" android:layout_toLeftOf="@+id/btn_delete" android:layout_toRightOf="@+id/ib_close" android:layout_centerVertical="true"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/temp" android:paddingTop="8dp" android:layout_width="match_parent" android:layout_height="64dp" android:layout_alignParentBottom="true">
            <ImageButton android:id="@+id/btn_phone" android:layout_width="192dp" android:layout_height="44dp" android:src="@drawable/phone_ic_call" android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="16dp" android:layout_marginRight="16dp" android:layout_above="@+id/temp" android:layout_below="@+id/layout_left_title">
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key1" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="1"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key2" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="2" app:key_desc="ABC"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key3" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="3" app:key_desc="DEF"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key4" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="4" app:key_desc="GHI"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key5" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="5" app:key_desc="JKL"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key6" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="6" app:key_desc="MNO"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key7" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="7" app:key_desc="PQRS"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key8" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="8" app:key_desc="TUV"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key9" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="9" app:key_desc="WXYZ"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="0dp" android:layout_weight="1">
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key_xing" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="*"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key0" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="0" app:key_desc="+"/>
                <com.baidu.carlife.view.softinputphonekey.SoftInputPhoneKey android:id="@+id/key_jing" android:focusable="true" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="1" app:key_name="#"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
