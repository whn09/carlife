<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@null" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/contain_frame" android:background="@null" android:layout_width="match_parent" android:layout_height="match_parent">
        <FrameLayout android:id="@+id/map_frame" android:background="@null" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <FrameLayout android:id="@+id/layout_carmode_side_bar" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_alignParentLeft="true">
            <include layout="@layout/carmode_side_bar"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/content_frame" android:background="@null" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_toRightOf="@+id/layout_carmode_side_bar"/>
        <RelativeLayout android:id="@+id/launch_view" android:background="@drawable/iv_launch" android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageView android:id="@+id/iv_anzhi_logo" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="25dp" android:layout_marginBottom="30dp" android:src="@drawable/anzhi_logo_ic" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <View android:id="@+id/forbid_touch_view" android:clickable="true" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
    <FrameLayout android:id="@+id/animImageMask_bg" android:background="#16191c" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="match_parent">
        <ImageView android:id="@+id/animImageMask" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <ImageView android:background="#b31d2026" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </FrameLayout>
    <com.baidu.navi.view.SlidingMenu android:id="@+id/right_menu" android:visibility="invisible" android:layout_width="250dp" android:layout_height="match_parent"/>
    <AbsoluteLayout android:id="@+id/animCanvas" android:background="#00000000" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
</RelativeLayout>
