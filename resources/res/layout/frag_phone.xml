<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/layout_phone" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/layout_title" android:layout_width="match_parent" android:layout_height="@dimen/common_item_height_64">
            <RelativeLayout android:id="@+id/btn_title_contacts" android:background="@drawable/common_btn_bg_focus" android:focusable="true" android:layout_width="@dimen/common_two_tap_width" android:layout_height="@dimen/common_item_height_64" android:layout_toRightOf="@+id/temp">
                <TextView android:textSize="18sp" android:textColor="@color/cl_text_a4_highlight_selector" android:id="@+id/tv_title_contacts" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/phone_title_contacts" android:layout_centerInParent="true"/>
                <View android:id="@+id/line_title_contacts" android:background="@color/cl_other_c_highlight" android:visibility="gone" android:layout_width="72dp" android:layout_height="2dp" android:layout_marginTop="8dp" android:layout_below="@+id/tv_title_contacts" android:layout_centerHorizontal="true"/>
            </RelativeLayout>
            <View android:id="@+id/temp" android:background="@color/cl_line_a3" android:layout_width="1dp" android:layout_height="20dp" android:layout_marginLeft="12dp" android:layout_marginRight="12dp" android:layout_toRightOf="@+id/btn_title_calllog" android:layout_centerVertical="true"/>
            <RelativeLayout android:id="@+id/btn_title_calllog" android:background="@drawable/com_bg_item_selector" android:focusable="true" android:layout_width="@dimen/common_two_tap_width" android:layout_height="@dimen/common_item_height_64" android:layout_marginLeft="76dp">
                <TextView android:textSize="18sp" android:textColor="@color/cl_text_a4_highlight_selector" android:id="@+id/tv_title_calllog" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/phone_title_calllog" android:layout_centerInParent="true"/>
                <View android:id="@+id/line_title_calllog" android:background="@color/cl_other_c_highlight" android:visibility="visible" android:layout_width="72dp" android:layout_height="2dp" android:layout_marginTop="8dp" android:layout_below="@+id/tv_title_calllog" android:layout_centerHorizontal="true"/>
            </RelativeLayout>
            <ImageButton android:id="@+id/btn_keyboard" android:layout_width="@dimen/phone_dial_voice_width" android:layout_height="@dimen/common_item_height_64" android:layout_marginRight="8dp" android:src="@drawable/phone_ic_dial" android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <com.baidu.carlife.view.CommonTipView android:id="@+id/common_tip_view" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/layout_title"/>
        <com.baidu.carlife.view.InterceptTouchRelativeLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="72dp" android:layout_marginRight="72dp" android:layout_below="@+id/layout_title">
            <ListView android:id="@+id/lv_calllog" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent" android:listSelector="@drawable/com_bg_item_selector" android:divider="@null"/>
            <ListView android:id="@+id/lv_contacts" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent" android:listSelector="@drawable/com_bg_item_selector" android:divider="@null"/>
        </com.baidu.carlife.view.InterceptTouchRelativeLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/btnlistview_left" android:visibility="invisible" android:layout_width="72dp" android:layout_height="match_parent" android:layout_below="@+id/layout_title" android:layout_alignParentLeft="true">
            <ImageButton android:id="@+id/btnlistview_btn_prev" android:background="@drawable/com_bg_item_selector" android:layout_width="match_parent" android:layout_height="48dp" android:layout_marginTop="8dp" android:src="@drawable/com_ic_prev" android:scaleType="center"/>
            <FrameLayout android:layout_gravity="center_horizontal" android:id="@+id/btnlistview_scroller" android:layout_width="2dp" android:layout_height="0dp" android:layout_weight="1">
                <FrameLayout android:id="@+id/btnlistview_scroll_bar" android:background="#33ffffff" android:layout_width="match_parent" android:layout_height="12dp"/>
            </FrameLayout>
            <ImageButton android:id="@+id/btnlistview_btn_next" android:background="@drawable/com_bg_item_selector" android:layout_width="match_parent" android:layout_height="48dp" android:src="@drawable/com_ic_next" android:scaleType="center"/>
        </LinearLayout>
    </RelativeLayout>
    <com.baidu.carlife.view.CommonTipView android:id="@+id/layout_nobt" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <RelativeLayout android:id="@+id/layout_incoming" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="28dp" android:textColor="@color/cl_text_a5_content" android:id="@+id/tv_incoming_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:maxWidth="400dp" android:singleLine="true" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="18sp" android:textColor="@color/cl_text_a2_content" android:id="@+id/tv_incoming_desc" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:text="@string/phone_state_incoming" android:layout_below="@+id/tv_incoming_name" android:layout_centerHorizontal="true"/>
        <RelativeLayout android:layout_width="424dp" android:layout_height="48dp" android:layout_marginBottom="40dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true">
            <ImageButton android:id="@+id/ib_incoming_answer" android:background="@drawable/com_bg_btn_d_selector" android:layout_width="192dp" android:layout_height="match_parent" android:src="@drawable/phone_ic_calling_call"/>
            <ImageButton android:id="@+id/ib_incoming_break" android:background="@drawable/com_bg_btn_c_selector" android:layout_width="192dp" android:layout_height="match_parent" android:src="@drawable/phone_ic_calling_hangup" android:layout_alignParentRight="true"/>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/layout_offhook" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="28dp" android:textColor="@color/cl_text_a5_content" android:id="@+id/tv_offhook_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:maxWidth="400dp" android:singleLine="true" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="18sp" android:textColor="@color/cl_text_a2_content" android:id="@+id/tv_offhook_desc" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:text="@string/phone_state_calling" android:layout_below="@+id/tv_offhook_name" android:layout_centerHorizontal="true"/>
        <Chronometer android:textSize="18sp" android:textColor="@color/cl_text_a2_content" android:id="@+id/view_offhook_start_timing" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:format="%s" android:layout_below="@+id/tv_offhook_name" android:layout_centerHorizontal="true"/>
        <ImageButton android:id="@+id/ib_offhook_break" android:background="@drawable/com_bg_btn_c_selector" android:layout_width="208dp" android:layout_height="48dp" android:layout_marginLeft="32dp" android:layout_marginRight="32dp" android:layout_marginBottom="40dp" android:src="@drawable/phone_ic_calling_hangup" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
        <ImageButton android:id="@+id/ib_offhook_voice" android:background="@drawable/phone_bg_btn_mute_selector" android:layout_width="56dp" android:layout_height="56dp" android:layout_marginBottom="36dp" android:src="@drawable/phone_ic_calling_mute" android:layout_toLeftOf="@+id/ib_offhook_break" android:layout_alignParentBottom="true"/>
        <ImageButton android:id="@+id/ib_offhook_keyboard" android:background="@drawable/com_bg_btn_selector" android:layout_width="56dp" android:layout_height="56dp" android:layout_marginBottom="36dp" android:src="@drawable/phone_ic_calling_dialpad" android:layout_toRightOf="@+id/ib_offhook_break" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <TextView android:textSize="16sp" android:textColor="@color/cl_text_a3_toast" android:id="@+id/phone_toast_tv" android:background="@drawable/shape_for_toast_text" android:paddingLeft="16dp" android:paddingTop="12dp" android:paddingRight="16dp" android:paddingBottom="12dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="256dp" android:text="@string/phone_multi_tip" android:layout_centerInParent="true"/>
</RelativeLayout>
