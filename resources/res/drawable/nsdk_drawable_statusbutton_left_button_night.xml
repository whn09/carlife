<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/nsdk_statusbutton_cl_bg_d_night"/>
            <stroke android:width="0.5dp" android:color="#33ffffff"/>
            <corners android:topLeftRadius="3dp" android:bottomLeftRadius="3dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/nsdk_statusbutton_cl_link_a_night"/>
            <corners android:topLeftRadius="3dp" android:bottomLeftRadius="3dp"/>
        </shape>
    </item>
</selector>
