<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="@color/cl_bg_c_main" android:endColor="@color/cl_bg_c_main" android:angle="0.0" android:centerY="0.75" android:centerColor="@color/cl_bg_c_main" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/cp_other_d" android:endColor="@color/cp_other_d" android:angle="0.0" android:centerY="0.75" android:centerColor="@color/cp_other_d" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="@color/cp_other_d" android:endColor="@color/cp_other_d" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>